generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Video {
  id                    Int               @id @default(autoincrement())
  title                 String
  description           String
  video                 String
  createdAt             DateTime          @default(now())
  updatedAt             DateTime          @default(now())
}
